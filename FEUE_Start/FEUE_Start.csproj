<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>

    <!-- Aktivera Windows Authentication -->
    <EnableWindowsAuthentication>true</EnableWindowsAuthentication>
    <AnonymousAuthentication>false</AnonymousAuthentication>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Authentication.Negotiate" Version="9.0.9" />
  </ItemGroup>

</Project>
